{
  "name": "takeoo/laravel-service-layer",
  "description": "Package enables service layer in all laravel applications. Move business logic to where it belongs!",
  "keywords": [
    "laravel",
    "service",
    "layer"
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "autoloader": {
    "psr4": {
      "Takeoo\\Service\\": "./src/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('./config/service.php') || copy('./vendor/takeoo/config/service.php', './config/service.php');\""
    ]
  }
}